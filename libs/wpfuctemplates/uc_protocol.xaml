<UserControl x:Class="wpfuctemplates.MyProtocolTeplate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:wpfuctemplates"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <GroupBox x:Name="box" Header="Protocol">
        <Grid>
            <RichTextBox x:Name="textObject" Background="Cornsilk" Margin="5"  FontSize="12"
            VerticalScrollBarVisibility="Visible" AcceptsReturn="True" IsReadOnly="True"/>
        </Grid>
    </GroupBox>
</UserControl>
